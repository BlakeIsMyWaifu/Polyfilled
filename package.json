{
  "name": "polyfilled",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:browser": "pnpm run open-browser",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "open-browser": "start http://localhost:3000",
    "eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "analyze": "cross-env ANALYZE=true pnpm run build",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "ts-coverage": "typescript-coverage-report"
  },
  "dependencies": {
    "next": "13.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "styled-components": "6.0.0-rc.3",
    "stylis": "^4.2.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.6",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@unchainedsky/eslint-config": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.6",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "5.1.3",
    "typescript-coverage-report": "^0.7.0"
  }
}